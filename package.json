{
  "name": "lazy-project",
  "version": "0.0.0-development",
  "description": "This is a project to show the lazy loading methods.",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server -r dotenv/config",
    "build:dev": "npm run clean && DOTENV_CONFIG_PATH=./.env webpack -r dotenv/config",
    "build:prod": "npm run clean && DOTENV_CONFIG_PATH=./.env.production webpack -r dotenv/config",
    "test": "npm run test:eslint && npm run test:sass-lint && npm run test:flow && npm run test:jest",
    "test:eslint": "eslint src/**/*.{js,jsx}",
    "test:sass-lint": "sass-lint -v -q",
    "test:flow": "flow",
    "test:jest": "jest",
    "test:majestic": "majestic",
    "test:jest:watch": "jest --watch",
    "panic": "rimraf package-lock.json && rimraf node_modules && npm install",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "precommit": "npm test",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "codecov": "codecov && cat ./coverage/lcov.info | codacy-coverage",
    "travis-deploy-once": "travis-deploy-once"
  },
  "keywords": [
    "lazy-loading"
  ],
  "author": "Gustavo Mu√±oz <timbergus@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@emotion/core": "^10.0.22",
    "@hot-loader/react-dom": "^16.11.0",
    "@loadable/component": "^5.11.0",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.7.1",
    "apollo-boost": "^0.4.7",
    "autoprefixer": "^9.7.3",
    "babel-plugin-universal-import": "^4.0.0",
    "codacy-coverage": "^3.4.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "^3.2.0",
    "majestic": "^1.6.1",
    "mini-css-extract-plugin": "^0.9.0",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.21",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.1.2",
    "react-spinners": "^0.7.0",
    "react-universal-component": "^4.0.0",
    "styled-components": "^4.4.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "codecov": "^3.6.1",
    "commitizen": "^4.0.3",
    "css-loader": "^3.4.0",
    "cz-conventional-changelog": "^3.0.2",
    "docz": "2.2.0",
    "docz-theme-default": "1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "flow-bin": "^0.115.0",
    "graphql-import-loader": "^0.2.1",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.0",
    "semantic-release": "17.2.3",
    "semantic-release-cli": "^5.2.3",
    "style-loader": "^1.1.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timbergus/lazy-project.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=13",
    "npm": ">=6"
  }
}
