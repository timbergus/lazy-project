{
  "name": "lazy-project",
  "version": "0.0.0-development",
  "description": "This is a project to show the lazy loading methods.",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server -r dotenv/config",
    "build:dev": "npm run clean && DOTENV_CONFIG_PATH=./.env webpack -r dotenv/config",
    "build:prod": "npm run clean && DOTENV_CONFIG_PATH=./.env.production webpack -r dotenv/config",
    "test": "npm run test:eslint && npm run test:sass-lint && npm run test:flow && npm run test:jest",
    "test:eslint": "eslint src/**/*.{js,jsx}",
    "test:sass-lint": "sass-lint -v -q",
    "test:flow": "flow",
    "test:jest": "jest",
    "test:majestic": "majestic",
    "test:jest:watch": "jest --watch",
    "panic": "rimraf package-lock.json && rimraf node_modules && npm install",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "precommit": "npm test",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "codecov": "codecov && cat ./coverage/lcov.info | codacy-coverage",
    "travis-deploy-once": "travis-deploy-once"
  },
  "keywords": [
    "lazy-loading"
  ],
  "author": "Gustavo Mu√±oz <timbergus@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@emotion/core": "^10.0.10",
    "@loadable/component": "^5.9.0",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^4.0.0-beta.1",
    "apollo-boost": "^0.4.0",
    "autoprefixer": "^9.5.1",
    "babel-plugin-universal-import": "^4.0.0",
    "codacy-coverage": "^3.4.0",
    "dotenv": "^8.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "^3.2.0",
    "majestic": "^1.4.1",
    "mini-css-extract-plugin": "^0.7.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.0.0",
    "react-spinners": "^0.5.4",
    "react-universal-component": "^4.0.0",
    "rimraf": "^2.6.3",
    "styled-components": "^4.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "codecov": "^3.4.0",
    "commitizen": "^3.1.1",
    "css-loader": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "docz": "1.2.0",
    "docz-theme-default": "1.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.8.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "flow-bin": "^0.100.0",
    "graphql-import-loader": "^0.2.1",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.1.0",
    "semantic-release": "^15.13.12",
    "semantic-release-cli": "^5.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timbergus/lazy-project.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
